{
  "name": "fastify-server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=development riteway -r ts-node/register/transpile-only __test__/**/*.test.ts | tap-nirvana",
    "cov": "cross-env NODE_ENV=development nyc --reporter=html --reporter=text-summary npm run test",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "cross-env NODE_ENV=development fastify start --port 3001 --ignore-watch=.ts$ -w -l info -P dist/src/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "@sinclair/typebox": "^0.23.5",
    "argon2": "^0.28.5",
    "fastify": "^4.0.0",
    "fastify-cli": "^4.1.1",
    "fastify-plugin": "^3.0.0",
    "nodemailer": "^6.7.5"
  },
  "devDependencies": {
    "@types/fs-ext": "^2.0.0",
    "@types/node": "^17.0.8",
    "@types/nodemailer": "^6.4.4",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "fastify-tsconfig": "^1.0.1",
    "fs-ext": "^2.0.0",
    "nyc": "^15.1.0",
    "riteway": "^6.3.1",
    "tap-nirvana": "^1.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.3"
  }
}
